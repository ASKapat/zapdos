#!/bin/bash

ZAPPROBDIR="/home/lindsayad/projects/zapdos/problems/"
INSTRING="mean_en_thermo"
ROOTSTRING=$INSTRING
OUTSTRING=$ROOTSTRING"_no_salt_or_H3Op_H_100000_zero_grad_mean_en"

