#!/bin/bash

ZAPPROBDIR="/home/lindsayad/projects/zapdos/problems/"
INSTRING="dens_kinetic_en_zero_grad_gamma_1e-4"
ROOTSTRING=$INSTRING
OUTSTRING=$ROOTSTRING

cp $ZAPPROBDIR"log.txt" $ZAPPROBDIR"MooseLogs/log_"$OUTSTRING"_gold.txt"
cp $ZAPPROBDIR$INSTRING".i" $ZAPPROBDIR$OUTSTRING"_gold.i"
cp $ZAPPROBDIR$INSTRING"_out.e" $ZAPPROBDIR"MooseOutput/"$OUTSTRING"_gold_out.e"
cp $ZAPPROBDIR"liquidKinetic.geo" $ZAPPROBDIR$OUTSTRING"_gold.geo"

